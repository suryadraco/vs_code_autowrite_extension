{
  "name": "human-writer",
  "displayName": "Human Writer",
  "description": "Simulates human-like typing when copying folder structures to a target location",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:humanWriter.writeFolder"
  ],
  
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "humanWriter.writeFolder",
        "title": "Human Write Folder To..."
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JAYASURYA-KK/human-writer"
  }
}


